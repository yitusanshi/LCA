<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.sys.dao.CalculateFeatureDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.sys.entity.CalculateFeatureEntity" id="calculateFeatureMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="feature11SecondId" column="feature_11_second_id"/>
        <result property="factor" column="factor"/>
        <result property="unit" column="unit"/>
        <result property="createdTime" column="created_time"/>
        <result property="excelOrder" column="excel_order"/>
    </resultMap>

    <select id="getById" parameterType="java.lang.Integer" resultMap="calculateFeatureMap">
        SELECT * FROM calculate_feature WHERE feature_11_second_id = #{0}
    </select>
    <select id="queryByIds" parameterType="java.util.List" resultMap="calculateFeatureMap">
      SELECT * FROM calculate_feature WHERE feature_11_second_id in
        <foreach collection="ids" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>

    <update id="update">
        UPDATE calculate_feature SET factor = #{factor} WHERE id = #{id}
    </update>
    
    <insert id=""
</mapper>